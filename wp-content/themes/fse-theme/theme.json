{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true
	},
	"styles": {
		"color": {
			"background": "#d5d6d7",
			"text": "#333"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "'Trebuchet MS', 'Lucida Grande', Verdana, Arial, Sans-Serif",
					"fontWeight": "600",
					"lineHeight": "1.2"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "#147"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				"color": {
					"text": "#06c"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"typography": {
			"fontFamily": "'Lucida Grande', Verdana, Arial, Sans-Serif",
			"fontSize": "13px",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.55"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "My Footer"
		}
	]
}